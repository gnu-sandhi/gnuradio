<?xml version="1.0"?>
<block>
  <name>Bode</name>
  <key>Bode</key>
  <category>Plots</category>
  <import>import gnuradio.input.Bode</import>
  <make>gnuradio.input.Bode.Bode($order)</make>

<param>
   <name>Order of transfer function</name>
   <key>order</key>
   <value>1</value>
   <type>real</type>

</param>



  <sink>
    <name>in</name>
    <type>float</type>
    <nports>2</nports>
</sink>

<doc>
	This block performs Bode plot of Transfer function defined by Numerator and Denominator block
	in0 = Numerator 
	in1 = Denominator
</doc>
</block>
